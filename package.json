    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.0",
    "@types/reselect": "^2.2.0",
    "@types/uuid": "^3.4.3",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",

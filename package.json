{
  "name": "discrelog",
  "version": "0.0.3",
  "description": "Logic simulator in React",
  "main": "dist/index.jsx",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "format": "prettier --spaces=2 --write \"src/**/*.{ts,tsx}\"",
    "deploy": "npm run build && git subtree push --prefix dist origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboPhred/discrelog.git"
  },
  "keywords": [
    "Discrete Logic",
    "Simulator",
    "Logic Simulator"
  ],
  "author": "William Matthews",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/platform": "^1.3.2",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^6.0.8",
    "@types/styled-components": "^4.1.8",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^7.1.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react-markdown": "^4.2.2",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "typescript-styled-plugin": "^0.6.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "immer": "^1.3.0",
    "konva": "^2.1.2",
    "lodash-es": "^4.17.15",
    "platform": "^1.3.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^3.0.5",
    "react-hotkeys": "^1.1.4",
    "react-konva": "^16.8.6",

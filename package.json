    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "format": "prettier --spaces=2 --write \"src/**/*.{ts,tsx}\"",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboPhred/discrelog.git"
  },
